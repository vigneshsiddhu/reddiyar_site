<p id="notice"><%= notice %></p>

<h1>Listing Family Members</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Relationship</th>
      <th>Age</th>
      <th>Email</th>
      <th>Gender</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @family_members.each do |family_member| %>
      <tr>
        <td><%= family_member.name %></td>
        <td><%= family_member.relationship %></td>
        <td><%= family_member.age %></td>
        <td><%= family_member.email %></td>
        <td><%= family_member.gender %></td>
        <td><%= link_to 'Show', family_member %></td>
        <td><%= link_to 'Edit', edit_family_member_path(family_member) %></td>
        <td><%= link_to 'Destroy', family_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Family member', new_family_member_path %>
